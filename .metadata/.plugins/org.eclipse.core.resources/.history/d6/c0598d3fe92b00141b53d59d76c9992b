package planpath;

import java.io.BufferedReader;
import java.io.File;
import java.io.FileInputStream;
import java.io.FileNotFoundException;
import java.io.FileReader;
import java.io.IOException;
import java.io.InputStreamReader;

public class Map {

	private FileInputStream file;
	private String content, row;
	private FileReader reader;

	public Map() throws IOException {

		this.file = new FileInputStream("input.txt");
		InputStreamReader stream = new  InputStreamReader(file);
		BufferedReader br = new BufferedReader(stream);
		String linha = br.readLine();
		System.out.println(linha.substring(0, linha.length()));

	}

	private String read() throws Exception {
		if (true) {
			System.out.println("ok");
			reader = new FileReader("input.txt");
			BufferedReader reader2 = new BufferedReader(reader);
			while (true) {
				row = reader2.readLine();
				if (row == null) {
					return content;
				}
				content += row + "\n";
			}
		}
		return null;
	}
	
	public static void main(String [] args) throws IOException{
		Map mapinha = new Map();
		try{
		System.out.println(mapinha.read());
		}
		catch(Exception e){
			
		}
	}

}
